		{
			"name": "Create New User",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"var random_digits = _.random(1000000, 9999999)\r",
							"var code = \"0168\"\r",
							"var nid_code = \"12\"\r",
							"var phone_number = code+random_digits\r",
							"var nid = nid_code+random_digits\r",
							"pm.environment.set(\"phone_number\", phone_number);\r",
							"pm.collectionVariables.set(\"phone_number\", phone_number);\r",
							"pm.collectionVariables.set(\"nidNumber\", nid);\r",
							"pm.environment.set(\"nidNumber\", nid);"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"User should create successfully\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.message).to.contains('User created successfully');\r",
							"});\r",
							"\r",
							"var jsonData=pm.response.json();\r",
							"var id=jsonData.user.id;\r",
							"pm.collectionVariables.set(\"id\", id);\r",
							"var countID = pm.collectionVariables.get(\"countID\");\r",
							"pm.collectionVariables.set(\"countID\", countID+1);\r",
							"\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
